{
  "name": "@openmrs/esm-form-entry-app",
  "version": "1.0.0-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:single-spa-local",
    "build:prod": "ng build",
    "test": "ng test --watch false",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:single-spa-local": "ng build --prod --output-hashing=none --deploy-url https://localhost:4200/",
    "build:single-spa-travis": "bash ./scripts/build-travis.sh",
    "serve:single-spa": "ng serve --ssl true  --disable-host-check --deploy-url https://localhost:4202/ --host localhost --port 4202 --live-reload true",
    "serve:dist": "npx angular-http-server -p 4200 --https --cors --path dist/",
    "build-serve:dist": "npm run build:single-spa-local && npm run serve:dist",
    "prettier": "prettier --config prettier.config.js --write \"src/**/*.{ts,tsx}\""
  },
  "private": true,
  "dependencies": {
