    "reflect-metadata": "^0.1.9",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "shelljs": "^0.7.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^4.9.2",
